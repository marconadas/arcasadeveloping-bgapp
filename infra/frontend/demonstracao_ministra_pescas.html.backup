                    performance: { api_response_time: 89.5 }
                },
                services: {
                    services: Array(12).fill({status: 'online'}).concat([{status: 'degraded'}])
                }
            };
            
            // Aplicar dados instantaneamente
            setTimeout(() => updateMetricsWithRealData(realData), 10);
        }

        // Inicializa√ß√£o otimizada
        document.addEventListener('DOMContentLoaded', function() {
            console.log('üöÄ BGAPP Demo carregado!');
            
            // Carregar m√©tricas instantaneamente
            loadMetricsInstantly();
            
            // Inicializar componentes de forma ass√≠ncrona para n√£o bloquear
            setTimeout(() => {
                initializeOceanSimulation();
            }, 50);
            
            setTimeout(() => {
                initializeMap();
                // startAdvancedAnimations(); // REMOVIDO - estava zerando as m√©tricas
                console.log('üé® Anima√ß√µes GSAP desabilitadas para preservar m√©tricas');
            }, 100);
            
            // Carregar dados reais em background
            setTimeout(() => {
                loadRealBGAPPData();
            }, 200);
            
            // Handle window resize for ocean simulation
            window.addEventListener('resize', onOceanResize);
            
            setTimeout(() => {
                showNotification('üåä Sistema BGAPP totalmente carregado!', 'success');
            }, 300);
        });

        // Tooltip interativo
        document.addEventListener('mousemove', function(e) {
            const tooltip = document.querySelector('.tooltip');
            if (tooltip) {
                tooltip.style.left = e.clientX + 15 + 'px';
                tooltip.style.top = e.clientY - 10 + 'px';
            }
        });

        // Efeitos de hover avan√ßados
        document.querySelectorAll('.capability-card').forEach(card => {
            card.addEventListener('mouseenter', function() {
                gsap.to(this, {
                    scale: 1.05,
                    duration: 0.3,
                    ease: "power2.out"
                });
            });
            
            card.addEventListener('mouseleave', function() {
                gsap.to(this, {
                    scale: 1,
                    duration: 0.3,
                    ease: "power2.out"
                });
            });
        });
    </script>
</body>
